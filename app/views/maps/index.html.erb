<h1>Listing maps</h1>

<table>
  <tr>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Gmaps</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @maps.each do |map| %>
  <tr>
    <td><%= map.latitude %></td>
    <td><%= map.longitude %></td>
    <td><%= map.gmaps %></td>
    <td><%= map.name %></td>
    <td><%= link_to 'Show', map %></td>
    <td><%= link_to 'Edit', edit_map_path(map) %></td>
    <td><%= link_to 'Destroy', map, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Map', new_map_path %>
